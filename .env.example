# .env.example

# Amplify AppSync 設定（API の GraphQL エンドポイント）
NEXT_PUBLIC_APPSYNC_URL=https://your-appsync-url

# Amplify AppSync の認証に使う API キー
NEXT_PUBLIC_APPSYNC_API_KEY=your-api-key

# リージョン（日本は通常 ap-northeast-1）
NEXT_PUBLIC_AWS_REGION=ap-northeast-1

# 初期データ投入ボタンを表示するかどうか
NEXT_PUBLIC_SHOW_SEED_BUTTON=true

